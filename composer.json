{
    "name": "bentools/cartesian-product",
    "description": "A simple, low-memory footprint function to generate all combinations from a multi-dimensionnal array.",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "BenTools\\CartesianProduct\\" : "src"
        },
        "files": ["src/function.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "BenTools\\CartesianProduct\\Tests\\" : "tests"
        },
        "files": [
            "vendor/symfony/var-dumper/Resources/functions/dump.php"
        ]
    },
    "require": {
        "php": ">=7.4"
    },
    "require-dev": {
        "symfony/var-dumper": "^6.0|^7.0",
        "phpunit/phpunit": "^9.0",
        "friendsofphp/php-cs-fixer": "^3.82"
    }
}
